{
	"name": "plancreator",
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"test:unit": "vitest",
		"test": "npm run test:unit -- --run",
		"vercel:link": "cross-env-shell \"vercel link --yes --token $VERCEL_TOKEN --scope etesie\"",
		"vercel:pull": "cross-env-shell \"vercel env pull --token $VERCEL_TOKEN --scope etesie\"",
		"init:repo": "pnpm config set auto-install-peers true && pnpm install && cross-env-shell \"vercel link --yes --token $VERCEL_TOKEN --scope etesie && vercel env pull --token $VERCEL_TOKEN --scope etesie\" && pnpm dev",
		"fauna:generate": "npx fauna-typed --dir='src/lib/db/schema/types'"
	},
	"devDependencies": {
		"@eslint/compat": "^1.2.5",
		"@skeletonlabs/skeleton": "3.0.0-next.10",
		"@skeletonlabs/skeleton-svelte": "1.0.0-next.17",
		"@sveltejs/adapter-auto": "^4.0.0",
		"@sveltejs/adapter-vercel": "^5.5.2",
		"@sveltejs/kit": "^2.15.3",
		"@sveltejs/vite-plugin-svelte": "^5.0.3",
		"@types/node": "^22.10.6",
		"autoprefixer": "^10.4.20",
		"cross-env": "^7.0.3",
		"eslint": "^9.18.0",
		"eslint-config-prettier": "^10.0.1",
		"eslint-plugin-svelte": "^2.46.1",
		"fauna-typed": "^0.3.4",
		"globals": "^15.14.0",
		"prettier": "^3.4.2",
		"prettier-plugin-svelte": "^3.3.3",
		"prettier-plugin-tailwindcss": "^0.6.9",
		"svelte": "^5.18.0",
		"svelte-check": "^4.1.4",
		"tailwindcss": "^3.4.17",
		"typescript": "^5.7.3",
		"typescript-eslint": "^8.20.0",
		"vite": "^6.0.7",
		"vite-plugin-mkcert": "^1.17.6",
		"vitest": "^2.1.8"
	},
	"dependencies": {
		"@inlang/paraglide-sveltekit": "^0.15.5",
		"@maizzle/framework": "^5.0.0",
		"@oslojs/binary": "^1.0.0",
		"@oslojs/crypto": "^1.0.1",
		"@oslojs/encoding": "^1.1.0",
		"@oslojs/webauthn": "^1.0.0",
		"@pilcrowjs/object-parser": "^0.0.4",
		"@tailwindcss/forms": "^0.5.10",
		"@tailwindcss/typography": "^0.5.16",
		"arctic": "^3.1.3",
		"fauna": "^2.4.1",
		"lucide-svelte": "^0.471.0",
		"resend": "^4.1.1",
		"tailwindcss-preset-email": "^1.4.0"
	}
}
