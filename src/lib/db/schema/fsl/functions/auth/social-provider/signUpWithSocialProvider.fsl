@role(server)
function signUpWithSocialProvider(sp: {firstName: String, lastName: String, email: String, avatar: String?, providerUserId: String, providerName: "Github" | "Facebook" | "Google", providerEmail: String}): { access: Token, refresh: Token } {
    
  createSocialProviderAccount(sp.firstName, sp.lastName, sp.email, sp.avatar, sp.providerName, sp.providerUserId, sp.providerEmail)
  signInWithSocialProvider(sp.providerName, sp.providerUserId)
}