@role(server)
function getPasskeyCredential(passkeyId: String): { id: String, userId: String, algorithmId: String, publicKey: String } | Null {

  let account = Account.firstWhere(.passkeyId == passkeyId)
  
  if(account != null) {
    {
      id: account!.passkeyId!,
      userId: account!.user.id.toString(),
      algorithmId: account!.algorithmId!,
      publicKey: account!.publicKey!
    }
  }
}