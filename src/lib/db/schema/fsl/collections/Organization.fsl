collection Organization {
    name: String
    logo: String?
    slug: String
    members: Array<{
        user: Ref<User>
        role: "role_organization_member" | "role_organization_admin" | "role_organization_owner"
    }>
    plan: "Free" | "Pro" | "Enterprise"
    compute invitations: Array<Invitation> = (org => Invitation.all().where(.organization == org).toArray())


    unique [.slug]
}