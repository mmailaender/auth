collection Organization {
    name: String
    logo: String?
    slug: String
    members: Array<{
        user: Ref<User>
        role: "role_organization_member" | "role_organization_admin" | "role_organization_owner"
    }>
    plan: "Free" | "Pro" | "Enterprise"

    compute invitations: Set<Invitation> | Null = (org => Invitation.all().where(.organization == org))

    unique [.slug]
}