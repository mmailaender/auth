@role(server)
function getPasskeyCredential(passkeyId: String): { id: String, userId: String, algorithmId: String, publicKey: String } | Null {

  let account = Account.firstWhere(.passkey?.id == passkeyId)
  
  if(account != null) {
    {
      id: account!.passkey!.id!,
      userId: account!.user.id.toString(),
      algorithmId: account!.passkey!.algorithmId!,
      publicKey: account!.passkey!.publicKey!
    }
  }
}