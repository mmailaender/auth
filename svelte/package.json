{
	"name": "auth",
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev:frontend": "vite dev",
		"dev:backend": "npx convex dev",
		"dev": "concurrently --kill-others \"pnpm dev:frontend\" \"pnpm dev:backend\"",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"test:unit": "vitest",
		"test": "npm run test:unit -- --run",
		"vercel:link": "cross-env-shell \"vercel link --yes --token $VERCEL_TOKEN --scope etesie\"",
		"vercel:pull": "cross-env-shell \"vercel env pull --token $VERCEL_TOKEN --scope etesie\"",
		"init:repo": "pnpm config set auto-install-peers true && pnpm install && cross-env-shell \"vercel link --yes --token $VERCEL_TOKEN --scope etesie && vercel env pull --token $VERCEL_TOKEN --scope etesie\" && pnpm dev",
		"pull:convex": "rm -rf ./src/convex && cp -r ../convex ./src/convex",
		"pull:convex-auth": "rm -rf ../dist/convex-auth && mkdir -p ../dist/convex-auth && cp -r ../../convex-auth/dist/* ../dist/convex-auth/"
	},
	"devDependencies": {
		"@eslint/compat": "^1.2.8",
		"@skeletonlabs/skeleton": "3.1.1",
		"@skeletonlabs/skeleton-svelte": "1.0.0",
		"@sveltejs/adapter-auto": "^6.0.0",
		"@sveltejs/adapter-vercel": "^5.6.3",
		"@sveltejs/kit": "^2.20.3",
		"@sveltejs/vite-plugin-svelte": "^5.0.3",
		"@tailwindcss/forms": "^0.5.10",
		"@tailwindcss/postcss": "^4.1.0",
		"@tailwindcss/typography": "^0.5.16",
		"@types/node": "^22.13.17",
		"@zag-js/file-upload": "^1.7.0",
		"concurrently": "^9.1.2",
		"cross-env": "^7.0.3",
		"eslint": "^9.23.0",
		"eslint-config-prettier": "^10.1.1",
		"eslint-plugin-svelte": "^3.5.0",
		"globals": "^16.0.0",
		"prettier": "^3.5.3",
		"prettier-plugin-svelte": "^3.3.3",
		"prettier-plugin-tailwindcss": "^0.6.11",
		"svelte": "^5.25.6",
		"svelte-check": "^4.1.5",
		"tailwindcss": "^4.1.0",
		"typescript": "^5.8.2",
		"typescript-eslint": "^8.29.0",
		"vite": "^6.2.4",
		"vite-plugin-mkcert": "^1.17.8",
		"vitest": "^3.1.1"
	},
	"dependencies": {
		"@auth/core": "^0.39.0",
		"@convex-dev/auth": "file:../../convex-auth",
		"@inlang/paraglide-sveltekit": "^0.16.1",
		"@maizzle/framework": "^5.0.6",
		"@oslojs/binary": "^1.0.0",
		"@oslojs/crypto": "^1.0.1",
		"@oslojs/encoding": "^1.1.0",
		"@oslojs/webauthn": "^1.0.0",
		"@pilcrowjs/object-parser": "^0.0.4",
		"@tailwindcss/vite": "^4.1.0",
		"@vercel/blob": "^0.27.3",
		"arctic": "^3.6.0",
		"arktype": "^2.1.16",
		"convex": "^1.22.0",
		"convex-ents": "^0.13.0",
		"convex-helpers": "^0.1.76",
		"convex-svelte": "^0.0.11",
		"fauna": "^2.5.0",
		"jose": "^6.0.10",
		"lucide-svelte": "^0.487.0",
		"resend": "^4.2.0",
		"sharp": "^0.33.5"
	}
}
